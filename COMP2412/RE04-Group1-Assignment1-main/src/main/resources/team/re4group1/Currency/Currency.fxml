<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="root"  prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="team.re4group1.CurrencyController">
  <children>
    <VBox prefHeight="${root.height}" prefWidth="${root.width}">
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="Auth">
            <items>
              <MenuItem fx:id="menuAuth" mnemonicParsing="false" text="Login" onAction="#onMenuAuthClick"/>
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <SplitPane dividerPositions="0.2" prefHeight="${root.height}" prefWidth="${root.width}">
        <items>
          <AnchorPane fx:id="splitPaneLeft" minHeight="0.0" minWidth="100" prefHeight="160.0" prefWidth="100.0">

          </AnchorPane>
          <AnchorPane fx:id="splitPaneRight" minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">

          </AnchorPane>
        </items>
      </SplitPane>
    </VBox>
  </children>
</AnchorPane>
